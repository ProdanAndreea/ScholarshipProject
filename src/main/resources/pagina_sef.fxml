<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="365.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.siemens.controller.PaginaSefController">
   <children>
      <ListView fx:id="requestListView" layoutY="85.0" prefHeight="211.0" prefWidth="600.0" />
      <Label layoutX="78.0" layoutY="1.0" prefHeight="27.0" prefWidth="149.0" text="Listă cereri învoiri">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="acceptButton" layoutX="77.0" layoutY="313.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="149.0" text="Acceptă Cerere">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Button fx:id="denyButton" layoutX="375.0" layoutY="313.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="149.0" text="Respinge Cerere">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <Label layoutX="233.0" layoutY="4.0" prefHeight="21.0" prefWidth="348.0" text="(dublu click pe cerere pentru a o deschide și semna)" textFill="#754747" />
      <Separator layoutY="0.5" prefHeight="0.0" prefWidth="600.0" />
      <ToggleButton fx:id="signedToggleButton" layoutX="267.0" layoutY="46.0" mnemonicParsing="false" text="Semnate" textFill="#058348">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></ToggleButton>
      <ToggleButton fx:id="waitingToggleButton" layoutX="109.0" layoutY="46.0" mnemonicParsing="false" text="În Așteptare" textFill="#c67d00">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></ToggleButton>
      <ToggleButton fx:id="resolvedToggleButton" layoutX="414.0" layoutY="46.0" mnemonicParsing="false" text="Rezolvate" textFill="#15009c">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></ToggleButton>
   </children>
</AnchorPane>
